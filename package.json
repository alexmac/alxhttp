{
  "name": "alxhttp",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "dependencies": {
    "@tanstack/react-query": "^5.56.2",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 200,
    "trailingComma": "all",
    "arrowParens": "always",
    "plugins": [
      "prettier-plugin-organize-imports",
      "prettier-plugin-tailwindcss"
    ],
    "tailwindAttributes": [
      "class",
      "className",
      "ngClass",
      ".*[cC]lassName"
    ],
    "tailwindFunctions": [
      "clsx",
      "cn"
    ]
  }
}
